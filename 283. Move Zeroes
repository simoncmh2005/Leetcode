class Solution {
    public void moveZeroes(int[] nums) {
        int left = 0;
        int right = nums.length-1;
        int [] result = new int[nums.length];
        for(int i = 0;i<nums.length;i++){
            if(nums[i]==0){
                result[right] = 0;
                right--;
            }else{
                result[left] = nums[i];
                left++;
            }
        }
        for(int i = 0; i < nums.length; i++) {
            nums[i] = result[i];
        }
    }
}
